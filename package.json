{
  "name": "ailearning-fullstack",
  "version": "1.0.0",
  "description": "Full-stack AI Learning Application",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm start",
    "client": "cd client && npm start",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\" --names \"SERVER,CLIENT\" --prefix-colors \"red,blue\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["react", "nodejs", "express", "mysql"],
  "author": "",
  "license": "ISC"
}
